    "# Get user input\n",
    "name = input(\"Enter your name: \")\n",
    "age = input(\"Enter your age: \")\n",
    "\n",
    "# Process the inputs\n",
    "greeting = f\"Hello, {name}!\"\n",
    "birth_year = 2023 - int(age)\n",
    "\n",
    "# Display the results\n",
    "print(greeting)\n",
    "print(f\"You were born in {birth_year}.\")\n"
